//A string is palindrome or not

Pseudofunctions

bool palindromeOrNot(str-->string)
temp = str
j = (str.length() - 1)
while (i < j)
    swap(str[i], str[j])
    i++
    j--
end while
if (temp == str)
    return true
end if
     return false


void palindromeOrNotTest()
input str
result = palindromeOrNot(str)
if (result)
   print "string is Palindrome"
else
   print "string is not Palindrome"


pseudocode
  palindromeOrNotTest()
  
