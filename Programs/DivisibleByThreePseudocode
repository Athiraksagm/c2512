// A number divisible by 3 or not
pseudofunction
function divisibleByThree(number-->int)
  if(number%3==0)
     return true
  end if
     return false
end function

function divisibleByThreeTest() 
  input number
  result=divisibleByThree(number)
  if(result)
     print "Number is divisible by three"
  else
     print "Number is not divisible by three"

pseudocode
   divisibleByThreeTest() 

  
